<template>
  <span>
    {{message}}
  </span>
</template>

<script>
  export default {
    name: 'HosSpan',
    props: ['code'],
    data() {
      return{
        message: ''
      }
    },
    mounted() {
      if(this.code){
        this.$get('/api/hospital/' + this.code).then(info => {
          this.message = info.hcoName
        }).catch(error => {
          this.message = ''
        })
      }

    },
    watch:{
      dictItemCode(){
        this.$get('/api/hospital/' + this.code).then(info => {
          this.message = info.hcoName
        }).catch(error => {
          this.message = ''
        })
      }
    }
  }
</script>
