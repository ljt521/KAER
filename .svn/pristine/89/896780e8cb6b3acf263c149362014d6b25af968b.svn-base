<template>
  <span>
    {{message}}
  </span>
</template>

<script>
  export default {
    name: 'DivSpan',
    props: ['dictCode', 'dictItemCode'],
    data() {
      return{
        message: ""
      }
    },
    mounted() {
      const data = {dictCode: this.dictCode, code:  this.dictItemCode, allowEnglish: false}
      if(this.dictItemCode){
        this.$get('/api/customer/getDictItemValue', {dictKey:this.dictCode,dictItemKey: this.dictItemCode}).then(info => {
          this.message = info;
        })
      }

    },
    methods: {
    },
    watch:{
      dictItemCode(){
        const data = {dictCode: this.dictCode, code:  this.dictItemCode, allowEnglish: false}
        this.$get('/api/customer/getDictItemValue', {dictKey:this.dictCode,dictItemKey: this.dictItemCode}).then(info => {
          this.message = info;
        })
      }
    }
  }
</script>
